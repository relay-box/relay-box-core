{
    "printWidth": 80,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "none",
    "proseWrap": "never",
    "tabWidth": 4,
    "semi": false,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ],
    "plugins": [
        "prettier-plugin-organize-imports",
        "prettier-plugin-packagejson"
    ]
}
